{
  "version":2,
  "builds": [
      {
          "src":"./index.py",
          "use": "@vercel/python",
          "config":{"runtime":"python3.8"}
      }
  ],
  "routes": [
      {
          "src":"/(.*)",
          "dest":"/",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {"Access-Control-Allow-Origin": "*"}
      }
  ]
}